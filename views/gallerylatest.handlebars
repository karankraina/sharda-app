<div class="container" style="margin-top:30px">
  <div class="head-text">
      <h1>Sharda Gallery</h1>
  </div>
  <div class="gallery-body">
      {{#each data}}
      <div class="gallery-card">
          <div class="gallery-card-top">
            <a href="{{{this.publicurl}}}" target="_blank"><img class="img-fluid gallery-img" src="{{{this.publicurl}}}" alt="{{{this.title}}}"></a>
          </div>
          
          <div class="gallery-card-bottom">
            <p>Art By: <span class="contri-name">{{this.contributor}}</span></p>
            <p>{{this.description}}</p>
          </div>
          <div class="gallery-card-text">
              
          </div>
      </div>
      {{/each}}
      
  </div>
</div>